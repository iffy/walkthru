{
    "manifest_version": 2,

    "name": "WalkThru Maker",
    "description": "Makes it easy to make webapp walkthroughs",
    "version": "1.0",

    "permissions": [
        "activeTab", "https://*/*", "http://*/*"
    ],
    "background": {
        "scripts": ["jquery.js", "background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "WalkThru Maker",
        "default_icon": "icon.png"
    }
    
}